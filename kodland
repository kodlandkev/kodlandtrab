import discord
from discord.ext import commands

description='''Este es un programa donde viculamos a discord con studio code para lanzar imagenes'''
intents=discord. Intents.default()
intents.members=True
intents.message_content=True

bot=commands.Bot(command_prefix='/', description=description, intents=intents)

@bot.event
async def on_ready():
    print(f'logeado como {bot.user} (ID: {bot.user.id}) ')

@bot.event
async def on_message(message):
    if message.author == bot.user:
        return
    if message.content.lower() == "/sendcat":
        with open('descarga.jpg', 'rb') as f:
            picture = discord.File(f)
            await message.channel.send(content='Aqui esta la imagen de tu gatito',file=picture)

bot.run("token")
